{
  "name": "@anthonypena/marked-better-image",
  "version": "2.0.0",
  "description": "Plugin to improve semantic rendering of images.",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "private": false,
  "scripts": {
    "clean": "rimraf ./dist",
    "build-ts": "tsup src/index.ts",
