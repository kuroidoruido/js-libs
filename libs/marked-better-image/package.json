    "@anthonypena/tsconfig": "workspace:*",
    "eslint": "^8.57.0",
    "marked": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsup": "^8.5.0",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "marked": "=16.x"
  },
  "prettier": "@anthonypena/prettier-config"
